pipeline {
    agent none
        stages {
                 stage ('build'){
                     agent { label 'node1'}
                     steps {
                             echo "this stage is running successfully"
                         }
                     }
                     stage ('deploy'){
                         agent {label 'node2'}
                         steps{
                             echo "test stage as been build successfully"
                         }
                     }
        
                }
        }
